{
  "name": "attrition-launcher",
  "version": "1.1.3",
  "description": "Attrition Game Launcher - Downloads, updates and launches the Attrition space strategy game",
  "main": "src/main.js",
  "author": "Attrition Game Studio",
  "license": "MIT",
  "homepage": "https://attrition-game.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrianSMitchell/attrition-launcher.git"
  },
  "bugs": {
    "url": "https://github.com/BrianSMitchell/attrition-launcher/issues"
  },
  "keywords": [
    "attrition",
    "game",
    "launcher",
    "electron",
    "space",
    "strategy"
  ],
  "scripts": {
    "dev": "electron .",
    "start": "electron .",
    "build": "electron-builder",
    "build:win": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --win",
    "build:win-nsis": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --win --config.win.target=nsis",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux",
    "dist": "electron-builder",
    "clean": "rimraf dist releases node_modules",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "electron-log": "^5.4.3",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^31.2.0",
    "electron-builder": "^24.13.3",
    "rimraf": "^5.0.5"
  }
}
